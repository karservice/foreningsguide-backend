{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Guide",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "introduction": {
      "type": "text",
      "required": true,
      "maxLength": 400
    },
    "order": {
      "type": "integer",
      "min": 1,
      "unique": true,
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "course-elements.course-text",
        "sections.html-utan-marginaler"
      ],
      "required": true
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "misc.seo",
      "required": true
    }
  }
}
